execute pathogen#infect()

syntax on

nnoremap <F8> :tabp<CR>
nnoremap <F10> :tabn<CR>

nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>

set mouse=a
set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab

set number

set hlsearch
map <Space> :nohlsearch<CR>

